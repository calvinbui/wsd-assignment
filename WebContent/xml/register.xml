<?xml version="1.0" encoding="UTF-8"?>

<registerform>

	<title>Register Driver</title>

	<register class="form-horizontal">
		<option class="form-group">
			<label for="username" class="control-label col-xs-2">Username</label>
			<input type="text" class="form-control" id="username"
				placeholder="Username" />
		</option>
		<option class="form-group">
			<label for="password" class="control-label col-xs-2">Password</label>
			<input type="text" class="form-control" id="password"
				placeholder="Password" />
		</option>
		<option class="form-group">
			<label for="confirmpassword" class="control-label col-xs-2">Confirm Password</label>
			<input type="date" class="form-control" id="confirmpassword"
				placeholder="Confirm password" />
		</option>
		<option class="form-group">
			<label for="firstname" class="control-label col-xs-2">First Name</label>
			<input type="date" class="form-control" id="firstname"
				placeholder="First name" />
		</option>
		<option class="form-group">
			<label for="lastname" class="control-label col-xs-2">Last Name</label>
			<input type="time" class="form-control" id="lastname"
				placeholder="Last name" />
		</option>
	</register>

</registerform>